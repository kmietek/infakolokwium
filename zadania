Zad2:
#include <iostream>
#include <cmath>

using namespace std;

int a,suma;
double pierwiastek;
int main()
/* BRZYDKO !!!! */
{
    do
    {
        cin >> a;
        suma = suma+a;
    } while (a!=0);
    cout << "suma = " << suma << endl;
    pierwiastek=sqrt(suma);
    cout << "pierwiastek = " << pierwiastek << endl;
    return 0;
}

