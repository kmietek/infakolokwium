Zad4:
#include <iostream>
#include <string>

using namespace std;


int main()
{
int x;
cout << "Podaj ilosc kolumn: " << endl;
cin >> x;
string T[x];
    for(int i=0;i<x;i++)
    {
        cin >> T[i];
    }
    cout << endl;
    for(int i=(x-1);i>-1;i--)
    {
        cout << T[i] << endl;
    }
return 0;
}
