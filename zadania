Zad 1: 
#include <iostream>

using namespace std;
int temp;

int main()
{
cout << "podaj temperature: " << endl;
cin >> temp;
if (temp<21)
    cout << "Grzanie" << endl;
    else if (temp>21)
        cout << "Chlodzenie" << endl;
    else cout << "Optymalna" << endl;
return 0;
}

