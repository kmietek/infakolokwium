Zad3:
#include <iostream>
#include <string>

using namespace std;

string T[5];
int main()
{
    cout << "Wprowadz dane do tabeli:" << endl;
    for (char i=0;i<5;i++)
    {
    cin >> T[i];
    }
    cout << "Liczba znakow 1: " << T[0].length() << endl;
    cout << "Liczba znakow 2: " << T[1].length() << endl;
    cout << "Liczba znakow 5: " << T[4].length() << endl;
return 0;
}

